            <Label x:Name="lbl_user" Content="Usuario" HorizontalAlignment="Left" Margin="26,17,0,0" VerticalAlignment="Top"/>
            <Label x:Name="lbl_pass" Content="Contraseña" HorizontalAlignment="Left" Margin="26,52,0,0" VerticalAlignment="Top"/>
            <Button x:Name="btn_admin" Content="Ingresar" HorizontalAlignment="Center" Margin="95,99,85,10" VerticalAlignment="Center" Width="75"/>
            <TextBox x:Name="txt_pass" HorizontalAlignment="Left" Height="23" Margin="110,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        </Grid>

    </Grid>
</Window>
