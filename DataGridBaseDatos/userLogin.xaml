<Window x:Name="Venana1" x:Class="userLogin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Consulta" Height="350" Width="525">
    <Window.Background>
        <ImageBrush ImageSource="images/cne-login.png"/>
    </Window.Background>
    <Grid>
        <Grid.Background>
            <ImageBrush/>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="89*"/>
            <RowDefinition Height="48*"/>
        </Grid.RowDefinitions>
        <Grid HorizontalAlignment="Center" Height="54" Margin="21,24,15,0" Grid.Row="1" Width="481" VerticalAlignment="Top">
            <TextBox x:Name="txtUser" Height="30" TextWrapping="Wrap" Width="228" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0"/>
            <Button x:Name="btn_ingresar" Content="Ingresar" Width="77" HorizontalAlignment="Right" Margin="0,10,0,14"/>
            <Label x:Name="label" Content="Cédula:" HorizontalAlignment="Left" Margin="40,5,0,0" VerticalAlignment="Top" FontSize="22"/>
        </Grid>

    </Grid>
</Window>
